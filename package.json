{
  "name": "discord-setlistfm",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@type-config/standard": "^1.1.1",
    "@types/lodash": "^4.14.165",
    "@types/node": "^16.11.7",
    "@types/ws": "^7.2.6",
    "axios": "^0.19.2",
    "discord-api-types": "^0.24.0",
    "discord.js": "https://github.com/thehackerboi69github/discord.js.git#b8dabca31eb1f99fad38f86c4623de4c9eee3c31",
    "dotenv": "^8.2.0",
    "fuse.js": "^6.4.0",
    "global": "^4.4.0",
    "knex": "^0.95.14",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.15",
    "pm2": "^4.4.0",
    "typescript": "^4.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.23",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.4",
    "jest": "^26.6.3",
    "mocha": "^8.4.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "nodemon": "^2.0.15"
  },
  "scripts": {
    "start": "yarn tsc && node dist/index.js",
    "register": "yarn tsc && node dist/deploy-commands.js",
    "scrape": "yarn tsc && node dist/scrape.js",
    "work": "yarn tsc && yarn run scrape-one",
    "scrape-one": "node dist/execute_artist_job.js",

    "watch": "./node_modules/nodemon/bin/nodemon.js",
    "dev": "yarn install && yarn run migrate && yarn run watch",

    "knex": "./node_modules/knex/bin/cli.js --knexfile knexfile.ts",
    "migrate": "yarn run knex migrate:latest",
    "rollback": "yarn run knex migrate:rollback",
    "remigrate": "yarn run rollback && yarn run migrate",

    "start-docker": "cd docker && docker-compose up -d",
    "attach-bot": "docker exec -it setlist_bot sh",
    "attach-scraper": "docker exec -it setlist_scraper sh",
    "logs-bot": "docker logs -tf setlist_bot",
    "logs-scraper": "docker logs -tf setlist_scraper",

    "test": "jest"
  }
}
